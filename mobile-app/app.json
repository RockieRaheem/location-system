{
  "expo": {
    "name": "Location System",
    "slug": "location-system-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#135bec"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.locationsystem.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#135bec"
      },
      "package": "com.locationsystem.mobile"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
